<h1>All Users</h1>
<h2><%= link_to 'New user', new_user_path %></h2>

<table>
  <tr>
    <th>Username:</th>
    <% @users.each do |user| %>
    <td><%= user.chief_username %></td>
    <td><%= link_to 'Show', user_path(user) %></td>
  </tr>
  <% end %>
</table>

<%= link_to 'Return to Welcome', controller: 'welcome' %>
