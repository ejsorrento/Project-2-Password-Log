
  <%= form_for @user do |f| %>





    <p>
      <%= f.label :website %><br>
      <%= f.text_field :website %>
    </p>

    <p>
      <%= f.label :assoc_email %><br>
      <%= f.text_area :assoc_email %>
    </p>

    <p>
      <%= f.label :assoc_password %><br>
      <%= f.text_field :assoc_password %>
    </p>

    <p>
      <%= f.label :user_id %><br>
      <%= f.text_field :user_id %>
    </p>


    <p>
      <%= f.submit %>
    </p>

  <% end %>
