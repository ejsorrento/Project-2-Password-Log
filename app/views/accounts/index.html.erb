<h1>All Accounts</h1>
<h2><%= link_to 'New account', new_user_account_path %></h2>

<table>
  <tr>
    <th>Website</th>

  </tr>

  <% @accounts.each do |account| %>
    <tr>
      <td><%= account.assoc_email %></td>
      <td><%= link_to 'Show', account_path(account) %></td>
    </tr>
  <% end %>
</table>

<td><%= link_to 'Return to User Info', users_path %></td>
